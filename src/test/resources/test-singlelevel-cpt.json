{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "five information requirements for users",
    "title": "身份证",
    "type": "object",
    "properties": {
        "weid": {
            "type": "string",
            "name": "微ID",
            "description": "user weId"
        },
        "age": {
            "description": "the age of certificate owner",
            "type": "integer",
            "name": "年龄"
        },
        "gender": {
            "description": "the gender of certificate owner",
            "enum": [
                "F",
                "M"
            ],
            "type": "string",
            "name": "性别"
        },
        "name": {
            "description": "the name of certificate owner",
            "type": "string",
            "name": "名字"
        }
    },
    "required": [
        "name",
        "age"
    ]
}