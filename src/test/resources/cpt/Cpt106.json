{
  "$schema" : "http://json-schema.org/draft-06/schema#",
  "type" : "object",
  "title": "Embedded Signature",
  "description": "Embedded Signature object for multi-sign",
  "properties" : {
    "credentialList" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "claim" : {
            "type" : "object"
          },
          "context" : {
            "type" : "string"
          },
          "cptId" : {
            "type" : "integer"
          },
          "expirationDate" : {
            "type" : "integer"
          },
          "id" : {
            "type" : "string"
          },
          "issuanceDate" : {
            "type" : "integer"
          },
          "issuer" : {
            "type" : "string"
          },
          "proof" : {
            "type" : "object"
          }
        }
      },
      "description": "Original credential list to be signed"
    }
  },
  "required": ["credentialList"]
}