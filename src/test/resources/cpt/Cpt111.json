{
  "$schema" : "http://json-schema.org/draft-06/schema#",
  "type" : "object",
  "title": "User CPT",
  "description": "User request issuer to sign credential",
  "properties" : {
    "cptId" : {
      "type" : "string"
    },
    "credentialSignatureRequest" : {
      "type" : "string",
      "description": "credential Signature Request",
      "minimum": 1
    },
    "userNonce" : {
      "type" : "string"
    }
  },
  "required": ["userNonce","credentialSignatureRequest","cptId"]
}